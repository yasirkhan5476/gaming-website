    <script type="text/javascript">
        var ratio = window.devicePixelRatio || 1;
        if(ratio == 1) {
            var width = window.screen.width;
            var height = window.screen.height;
        }
        else {
            var width = Math.floor(window.screen.width * ratio);
            var height = Math.floor(window.screen.height * ratio);
        }

        if(parseInt(width.toString().split('').pop()) == 1 && parseInt(height.toString().split('').pop()) == 1) {
            width -= 1;
            height -= 1;
        }
        //Apple iPhone 6+, 6s+, 7+, 8+
        else if(width == 1242 && height == 2208 && ratio == 3) {
            width = 1080;
            height = 1920;
        }

        $('#crop_stretch_options').replaceWith("<a rel='nofollow' class='btn btn-info' href=\"https://wall.alphacoders.com/crop.php?id=1305528&amp;w=" + width + "&amp;h=" + height + "\">" + width + "x" + height + " Cropped</a>" +
            "<a rel='nofollow' class='btn btn-info' href=\"https://wall.alphacoders.com/crop.php?id=1305528&amp;w=" + width + "&amp;h=" + height + "&amp;type=stretch\">" + width + "x" + height + " Stretched</a>");
    </script>

